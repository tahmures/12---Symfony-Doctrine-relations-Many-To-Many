{% extends 'base.html.twig' %}

{% block title %}Hello TagController!{% endblock %}

{% block body %}
    <div class="example-wrapper">
        <h1>{{ tag.name }}</h1>
        <ul>
            {% for article in articles %}
                <li>{{ article.content }}</li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}